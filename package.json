{
  "name": "enspiraled",
  "version": "1.1.0",
  "description": "Circles on circles on circles, the React way.",
  "repository": "https://github.com/dev-academy-challenges/enspiraled.git",
  "main": "server",
  "scripts": {
    "start": "run-p server webpack",
    "dev": "run-p server client",
    "server": "node server",
    "client": "npm run webpack -- --watch",
    "webpack": "webpack --config ./client/webpack.config.js",
    "test": "jest --watch --noStackTrace",
    "lint": "eslint --ext .js,.jsx ."
  },
  "author": "EDA",
  "license": "MIT",
  "keywords": [
    "frontend"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "eslint": "^8.8.0",
    "eslint-config-eda": "^1.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
